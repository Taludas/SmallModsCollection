            <Item>
              <Filename>data/modgraphics/models/cinnamon_tree/cinnamon_tree_01.prp</Filename>
            </Item>
          </Variations>
        </Object>
        <Prop>
          <HerdBlocking>PassableWhenSparse</HerdBlocking>
          <VegetationType>Tree</VegetationType>
        </Prop>
        <Constructable>
          <ConstructionMode>Tree</ConstructionMode>
        </Constructable>
        <Text>
          <LocaText>
            <English>
              <Text>Cinnamon Grove</Text>
              <Status>Exported</Status>
              <ExportCount>1</ExportCount>
            </English>
          </LocaText>
          <LineID>50482</LineID>
        </Text>
        <Locked />
        <Cost>
          <Costs>
            <Item>
              <VectorElement>
                <InheritedIndex>0</InheritedIndex>
